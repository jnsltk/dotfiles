music_directory "/home/aro/music"
playlist_directory "/home/aro/.mpd/playlists"
db_file "/home/aro/.mpd/mpd.db"
log_file "/home/aro/.mpd/mpd.log"
pid_file "/home/aro/.mpd/mpd.pid"
state_file "/home/aro/.mpd/mpdstate"
bind_to_address "127.0.0.1"
port "6600"
user "aro"
audio_output {  
	type "alsa"
	name "MPD"
	device "pulse"
	mixer_control "Master"
	server "localhost"
	sink "alsa_output.pci-0000_00_1b.0.analog-stereo"
}
